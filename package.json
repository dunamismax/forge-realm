{
  "name": "forge-realm",
  "version": "4.0.0",
  "description": "Hugo website for Forge Realm - Open Source Trading Card Game",
  "main": "index.js",
  "homepage": "https://forge-realm.com",
  "scripts": {
    "dev": "hugo server --buildDrafts --bind 0.0.0.0",
    "build": "hugo mod tidy && hugo --minify --environment production",
    "build:preview": "hugo --buildDrafts --environment staging",
    "clean": "rm -rf public resources",
    "serve": "hugo server --environment production --disableFastRender",
    "cf-pages": "npm run build",
    "validate:build": "npm run build",
    "test": "npm run validate:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dunamismax/forge-realm.git"
  },
  "keywords": [],
  "author": "dunamismax@forge-realm.com",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/dunamismax/forge-realm/issues"
  },
  "homepage": "https://github.com/dunamismax/forge-realm#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
